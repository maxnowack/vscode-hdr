{
  "name": "hdr",
  "displayName": "HDR",
  "description": "Enables HDR (High Dynamic Range) colors on supported devices",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hdr.toggle",
        "title": "HDR: Toggle"
      },
      {
        "command": "hdr.installPlugin",
        "title": "HDR: Install"
      },
      {
        "command": "hdr.uninstallPlugin",
        "title": "HDR: Uninstall"
      }
    ],
    "configuration": {
      "title": "HDR",
      "properties": {
        "hdr.enable": {
          "description": "Enable HDR mode",
          "type": "boolean"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/uuid": "^9.0.2",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
